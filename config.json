{
  "pretrained": "Salesforce/codet5-base",
  "data_dir": "data/",
  "train_data": "labeled/train.jsonl",
  "valid_data": "labeled/test.jsonl",
  "test_data": "labeled/test.jsonl",
  "data_loader": {
    "args": {
      "batch_size": 16,
      "shuffle": true,
      "num_workers": 2
    }
  },
  "optimizer": {
    "type": "Adam",
    "args":{
      "lr": 1e-7
    }
  },
  "metrics": [],
  "trainer": {
    "epochs": 10000,
    "save_dir": "saved/0831.productions_only/",
    "save_period": 100,
    "valid_period": 100
  },
  "generate": {
    "unlabeled_data": "unlabeled/code_contests_train_python.jsonl",
    "output": "generated_grammar.jsonl",
    "args": {
      "length_penalty": 1.0,
      "max_new_tokens": 150,
      "num_beams": 10,
      "repetition_penalty": 2.5,
      "early_stopping": true
    }
  },
  "execute": {
    "solution_dir": "data/solutions/"
  }
}
