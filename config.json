{
  "pretrained": "Salesforce/codet5-base",
  "data_dir": "data/",
  "train_data": "labeled/train.jsonl",
  "valid_data": "labeled/test.jsonl",
  "test_data": "labeled/test.jsonl",
  "unlabeled_train_data": "unlabeled/code_contests_train_python.jsonl",
  "unlabeled_test_data": "unlabeled/code_contests_test_python.jsonl",
  "data_loader": {
    "args": {
      "batch_size": 8,
      "shuffle": true,
      "num_workers": 4
    }
  },
  "optimizer": {
    "type": "Adam",
    "args":{
      "lr": 1e-7
    }
  },
  "metrics": [],
  "trainer": {
    "epochs": 1000,
    "save_dir": "saved/pseudo_labeling/",
    "save_period": 50,
    "valid_period": 50,
    "pseudo_label_period": 1
  },
  "test": {
    "model_dir": null
  },
  "generation_config": {
    "length_penalty": 1.0,
    "max_new_tokens": 150,
    "num_beams": 5,
    "repetition_penalty": 2.5,
    "early_stopping": true,
    "num_return_sequences": 1
  },
  "generate": {
    "unlabeled_data": "unlabeled/code_contests_train_python.jsonl",
    "output": "generated_grammar.jsonl",
    "model_dir": "saved/0905/"
  },
  "execute": {
    "solution_dir": "data/solutions/"
  }
}
